<!DOCTYPE html>
<html lang="es">
    <head>
        <meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />	
		<meta name="keywords" content="pickmystuff">
		<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
		<meta name="apple-mobile-web-app-capable" content="yes" />
		<meta name="mobile-web-app-capable" content="yes">
        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
        
        <title>{% block title %}Pick My Stuff {% endblock %}</title>
        
	    <!-- stylesheets -->
	    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>
	    <link href='http://fonts.googleapis.com/css?family=Roboto:300,400,700' rel='stylesheet' type='text/css'>
	    
		{# stylesheets... #}
		{% block stylesheets %}
		{% stylesheets output='css/frontend.min.css' filter='cssrewrite'
			'bundles/odiseopickmystuff/css/bootstrap.min.css'
			'bundles/odiseopickmystuff/css/main.css'
		%}
		<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
		{% endstylesheets %}
		{% endblock %}
    </head>
    <body>
    	<div class="container wrapper">
    		{% block header %}
    		<header>
	    		<nav class="navbar navbar-default" role="navigation">
  					<div class="container-fluid">
  						<div class="nav navbar-nav navbar-left">
  							<a class="logo" href="#"><img class="img-responsive" src="{{ asset('bundles/odiseopickmystuff/images/logo.png') }}"></a>
  						</div>
		    			<!-- Brand and toggle get grouped for better mobile display -->
							<ul class="nav navbar-nav navbar-right">
	      					  <li class="active about"><a href="{{ path('odiseo_pick_my_stuff_frontend_home_aboutus') }}">About Us</a></li>
	      					  <li class="services"><a href="">Services</a></li>
	      					  <li class="contactus"><a href="{{ path('odiseo_pick_my_stuff_frontend_home_contact') }}">Contact Us</a></li>
	      					  <li class="faq"><a href="#">FAQ</a></li>
	      					  <li><a class="facebook" href="#"><img class="img-responsive" src="{{ asset('bundles/odiseopickmystuff/images/facebook-icon.png') }}"></a></li>
	      					  <li><a class="twitter" href="#"><img class="img-responsive" src="{{ asset('bundles/odiseopickmystuff/images/twitter-icon.png') }}"></a></li>
							</ul>      					  
		    			</div>
		    	</nav> 
    		</header>
    		{% endblock %}
    		<div id="content">
				{% block content %}
				{% endblock %}
			</div>
			<footer>
				<div class="separator"></div>
				<div class="footer"></div>
			</footer>
		</div>
		
		<!-- javascript -->
		{# javascripts... #}
		{% block javascripts %}
		<script type="text/javascript" src="https://code.jquery.com/jquery-1.11.1.min.js"></script>
		{% javascripts output='js/frontend.min.js'
		   'bundles/odiseopickmystuff/js/vendor/bootstrap-filestyle.min.js'
		   'bundles/odiseopickmystuff/js/vendor/bootstrap.min.js'
		   'bundles/odiseopickmystuff/js/main.js'	
		%}
		<script type="text/javascript" src="{{ asset_url }}"></script>
		{% endjavascripts %}
		{% endblock %}
    </body>
</html>
